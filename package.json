{
  "name": "@alphaxp/base",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "14",
    "yarn": "1"
  },
  "scripts": {
    "clean": "rm -rf dist **/.cache *.log",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "start": "node ./server/index.js",
    "format": "prettier --write '**/*.{css,html,js,json,md,ts,tsx,yaml}'",
    "lint": "yarn lint:css && yarn lint:js && yarn lint:types",
    "lint:css": "stylelint --fix '**/*.css'",
    "lint:js": "eslint --fix --ext .js,.ts,.tsx ./",
    "lint:types": "tsc --noEmit",
    "pretest": "yarn lint",
    "test": "jest",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "dependencies": {
    "debug": "4.3.3",
    "express": "4.17.2",
    "node-fetch": "3.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@heroicons/react": "1.0.5",
    "@storybook/addon-actions": "6.4.16",
    "@storybook/addon-essentials": "6.4.16",
    "@storybook/addon-links": "6.4.16",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/react": "6.4.16",
    "@svgr/webpack": "6.2.0",
    "@tailwindcss/forms": "0.4.0",
    "@tailwindcss/typography": "0.5.1",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.13",
    "@types/node-fetch": "3.0.3",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "autoprefixer": "10.4.2",
    "babel-loader": "8.2.3",
    "cssnano": "5.0.16",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "jest-playwright-preset": "1.7.0",
    "nock": "13.2.2",
    "playwright": "1.18.1",
    "postcss": "8.4.5",
    "postcss-preset-env": "7.2.3",
    "prettier": "2.5.1",
    "prettier-plugin-tailwindcss": "0.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "2.66.1",
    "stylelint": "14.3.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "24.0.0",
    "stylelint-order": "5.0.0",
    "supertest": "6.2.2",
    "tailwindcss": "3.0.18",
    "ts-jest": "27.1.3",
    "tsm": "2.2.1",
    "typescript": "4.5.5",
    "vite": "2.7.13"
  }
}
